{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "build",
			"path": "esm-samples/jsapi-angular-cli/",
			"group": "build",
			"problemMatcher": [],
			"label": "npm: build - esm-samples/jsapi-angular-cli",
			"detail": "ng build"
		},
		{
			"type": "npm",
			"script": "start",
			"path": "esm-samples/jsapi-angular-cli/",
      "isBackground": true,
      "presentation": {
        "focus": true,
        "panel": "dedicated"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": {
        "owner": "typescript",
        "source": "ts",
        "applyTo": "closedDocuments",
        "fileLocation": ["relative", "${cwd}"],
        "pattern": "$tsc",
        "background": {
          "activeOnStart": true,
          "beginsPattern": {
            "regexp": "(.*?)"
          },
          "endsPattern": {
            "regexp": "Compiled |Failed to compile."
          }
        }
      },
			"label": "npm: start - esm-samples/jsapi-angular-cli",
			"detail": "ng serve"
		}
	]
}